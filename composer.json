{
  "name": "Jobyh/laravel-react-make",
  "description": "Artisan command to generate React components",
  "keywords": [
    "Jobyh",
    "laravel",
    "react",
    "artisan",
    "generator"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Joby Harding",
      "email": "joby@jobyharding.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Jobyh\\ReactMake\\": "src"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "^7.2"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Jobyh\\ReactMake\\Support\\CommandsProvider"
      ]
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0|^9.0",
    "orchestra/testbench": "^3.8|^4.0|^5.0"
  }
}
